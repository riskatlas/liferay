<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>layman-portlet-gray</portlet-name>
		<icon>/icon.png</icon>
        <configuration-action-class>
        	cz.ccss.layman.ConfigurationActionImpl
        </configuration-action-class>
		<instanceable>false</instanceable>
        <header-portal-css>/wwwlibs/ext4/resources/css/ext-sandbox-gray.css</header-portal-css>
        <header-portal-javascript>/wwwlibs/ext4/builds/ext-all-sandbox.js</header-portal-javascript>
        <header-portal-javascript>/wwwlibs/proj4js/proj4js.js</header-portal-javascript>
        <header-portal-javascript>/wwwlibs/proj4js/projCode/merc.js</header-portal-javascript>
        <header-portal-javascript>/wwwlibs/hslayers/OpenLayers.js</header-portal-javascript>
        <header-portal-javascript>/js/layman/lib/HSRS.js</header-portal-javascript>
	</portlet>
	<portlet>
		<portlet-name>layman-portlet-blue</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
        <header-portal-css>/wwwlibs/ext4/resources/css/ext-sandbox.css</header-portal-css>
        <header-portal-javascript>/wwwlibs/ext4/builds/ext-all-sandbox.js</header-portal-javascript>
        <header-portal-javascript>/wwwlibs/proj4js/proj4js.js</header-portal-javascript>
        <header-portal-javascript>/wwwlibs/hslayers/OpenLayers.js</header-portal-javascript>
        <header-portal-javascript>/js/layman/lib/HSRS.js</header-portal-javascript>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>
